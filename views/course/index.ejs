<div class="container">
    <div class="row">
        <div class="col-md-12">
            
    <h1 class="mt-3">List of courses</h1>
    
    <div class="jumbotron">
        <h1 class="display-3">Formation on NodeJS</h1>
        <p class="lead">Nodejs for developer on atos origin casa nearshore</p>
        <hr class="my-2">
        <p>More info</p>
        <p class="lead">
            <a class="btn btn-primary btn-lg" href="Jumbo action link" role="button">Jumbo action name</a>
        </p>
    </div>
    <hr>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>Author</th>
                <th>Price</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% courses.forEach(function(course){ %>
            <tr>
                <td><%= course.name; %></td>
                <td><%= course.author; %></td>
                <td><%= course.price; %></td>
                <td class="text-right"><a href="" class="btn btn-warning">Edit</a>
                    <a href="" class="btn btn-danger">Delete</a></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
